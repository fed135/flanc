{
  "name": "<project_name>",
  "version": "0.0.0",
  "description": "Website api",
  "private": true,
  "workspaces": [
    "packages/*",
    "packages/domain/*",
    "packages/resource/*",
    "packages/router/*",
    "test/*"
  ],
  "main": "index.js",
  "engines": {
    "node": "^14"
  },
  "scripts": {
    "watch": "nodemon $npm_package_main",
    "start": "node $npm_package_main",
    "lint": "eslint --ext .ts --ext .js .",
    "lint:fix": "eslint --ext .ts --ext .js . --fix",
    "test": "yarn test:unit && yarn test:mid-level",
    "test:acceptance": "NODE_ENV=qa AWS_REGION=ca-central-1 jest test/acceptance/* --forceExit --maxWorkers=4",
    "test:coverage": "NODE_ENV=qa AWS_REGION=ca-central-1 jest packages/* --forceExit --maxWorkers=4 --coverage",
    "test:mid-level": "NODE_ENV=qa AWS_REGION=ca-central-1 jest test/mid-level/* --forceExit --maxWorkers=4",
    "test:unit": "NODE_ENV=qa AWS_REGION=ca-central-1 jest packages/* --forceExit --maxWorkers=4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/get<project_name>/<project_name>.git"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint",
      "post-merge": "yarn"
    }
  },
  "typings": "<project_name>.d.ts",
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/get<project_name>/<project_name>/issues"
  },
  "homepage": "https://github.com/get<project_name>/<project_name>#readme",
  "dependencies": {
    "@<project_name>/core": "0.0.0",
    "config": "^3.3.0",
    "dotenv": "^8.2.0",
    "ha-dns-cache": "^1.0.1",
    "newrelic": "^6.11.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^26.0.0",
    "@types/mongodb": "^3.5.27",
    "@types/newrelic": "^6.13.0",
    "@types/node": "^14.0.0",
    "@types/redis": "^2.8.28",
    "@types/stoppable": "^1.1.0",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "husky": "^4.2.5",
    "jest": "^26.2.0",
    "jest-when": "^2.7.2",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0"
  }
}
