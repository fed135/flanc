      "@types/jest": "^26.0.0",
      "@types/node": "^15.0.0",
      "@types/stoppable": "^1.1.0",
      "@types/supertest": "^2.0.10",
      "@types/uuid": "^8.3.0",
      "@typescript-eslint/eslint-plugin": "^4.22.0",
      "@typescript-eslint/parser": "^4.22.0",
      "eslint": "^7.25.0",
      "eslint-config-airbnb-base": "^14.2.0",
      "eslint-plugin-import": "^2.22.0",
      "eslint-plugin-jest": "^24.3.0",
