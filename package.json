{
    "name": "flanc-project",
    "version": "0.0.0",
    "description": "Website api",
    "private": true,
    "workspaces": [
      "packages/*",
      "packages/router/*",
      "test/*"
    ],
    "main": "index.js",
    "engines": {
      "node": "^14"
    },
    "scripts": {
      "lint": "eslint --ext .ts --ext .js .",
      "lint:fix": "eslint --ext .ts --ext .js . --fix",
      "test": "yarn test:unit && yarn test:integration",
      "test:integration": "NODE_ENV=qa jest test/integration/* --forceExit --maxWorkers=4",
      "test:unit": "NODE_ENV=qa jest packages/* --forceExit --maxWorkers=4"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/fed135/flanc.git"
    },
    "husky": {
      "hooks": {
        "pre-push": "yarn lint",
      }
    },
    "typings": "index.d.ts",
    "author": "Frederic Charette <fredericcharette@gmail.com>",
    "license": "Apache 2.0",
    "bugs": {
      "url": "https://github.com/fed135/flanc/issues"
    },
    "homepage": "https://github.com/fed135/flanc#readme",
    "devDependencies": {
      "@types/express": "^4.17.2",
      "@types/jest": "^26.0.0",
      "@types/mongodb": "^3.5.27",
      "@types/newrelic": "^6.13.0",
      "@types/node": "^14.0.0",
      "@types/redis": "^2.8.28",
      "@types/stoppable": "^1.1.0",
      "@types/supertest": "^2.0.10",
      "@types/uuid": "^8.0.0",
      "@typescript-eslint/eslint-plugin": "^3.7.0",
      "@typescript-eslint/parser": "^3.7.0",
      "eslint": "^7.5.0",
      "eslint-config-airbnb-base": "^14.2.0",
      "eslint-plugin-import": "^2.22.0",
      "eslint-plugin-jest": "^23.20.0",
      "husky": "^4.2.5",
      "jest": "^26.2.0",
      "jest-when": "^2.7.2",
      "supertest": "^4.0.2",
      "ts-jest": "^26.1.0",
      "ts-node": "^8.10.1",
      "typescript": "^3.9.7"
    }
  }
  