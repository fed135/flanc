      "packages/router/*"
    ],
    "engines": {
      "node": ">=14"
    },
    "scripts": {
      "lint": "eslint --ext .ts --ext .js .",
      "lint:fix": "eslint --ext .ts --ext .js . --fix",
      "test": "yarn workspaces run test"
